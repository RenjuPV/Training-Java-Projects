<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
	<display-name>131-Customers-CURD-Web</display-name>
	
	<servlet>
		<servlet-name>FrontController</servlet-name>
		<servlet-class>com.training.ui.FrontControllerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FrontController</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ListCustomersController</servlet-name>
		<servlet-class>com.training.ui.ListCustomersControllerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListCustomersController</servlet-name>
		<url-pattern>/ListCustomers</url-pattern>
	</servlet-mapping>
	
	
</web-app>